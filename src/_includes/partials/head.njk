<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>{{ page_title }}</title>
<meta name="description" content="{{ description }}">
<meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
<link rel="apple-touch-icon" href="{{ relative_path }}/60x60.png">
<link rel="apple-touch-icon" sizes="76x76" href="{{ relative_path }}/76x76.png">
<link rel="apple-touch-icon" sizes="120x120" href="{{ relative_path }}/120x120.png">
<link rel="apple-touch-icon" sizes="152x152" href="{{ relative_path }}/152x152.png">
<link rel="shortcut icon" href="{{ relative_path }}/favicon.ico" type="image/x-icon">
<link rel="icon" href="{{ relative_path }}/favicon.png">
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link rel="preload" as="font" type="font/woff2" href="https://fonts.gstatic.com/s/inter{{fontVersions[0]}}UcC73FwrK3iLTeHuS_nVMrMxCp50SjIa25L7W0Q5n-wU.woff2" crossorigin="true">
<link rel="preload" as="font" type="font/woff2" href="https://fonts.gstatic.com/s/playfairdisplay{{fontVersions[1]}}nuFiD-vYSZviVYUb_rj3ij__anPXDTLYgEM86xRbPQ.woff2" crossorigin="true">
<link href="https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;900&family=Playfair+Display:wght@400;700;900&display=swap" rel="stylesheet">
<link type="text/plain" rel="author" href="{{ relative_path }}/humans.txt" />
{# Open Graph / Facebook #}
<meta property="og:title" content="{{ page_title }}"/>
<meta property="og:description" content="{{ description }}"/>
<meta property="og:type" content="website"/>
<meta property="og:url" content="{{ site.siteBase }}"/>
<meta property="og:image" content="{{ site.siteBase }}/og-image.jpg"/>
<meta property="og:image:width" content="1200"/>
<meta property="og:image:height" content="630"/>
{# Twitter #}
<meta property="twitter:card" content="summary_large_image" />
<meta property="twitter:url" content="{{ site.siteBase }}" />
<meta property="twitter:title" content="{{ page_title }}" />
<meta property="twitter:description" content="{{ description }}" />
<meta property="twitter:image" content="{{ site.siteBase }}/og-image.jpg" />
</head>